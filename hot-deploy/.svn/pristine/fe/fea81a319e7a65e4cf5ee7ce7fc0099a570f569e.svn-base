	<script type="text/javascript">
		require(["jquery", "logisticsdata", "bootstrap", "domReady!"], function($, logisticsdata){
			// Logics within the application page
			var data = logisticsdata.deliver;
			var querytems = logisticsdata.querytems;
			var months = logisticsdata.months;
			var otherorders = logisticsdata.otherorders;
			var typemap = logisticsdata.typemap;
			var statusmap = logisticsdata.statusmap;

			var curorder = null;

			var currentview = "";

			var changestatus = function(status){
				status = statusmap[status];
				$(".statusbar").removeClass("oe_active");
				$("#status-" + status).addClass("oe_active");
			};
			var fillnew = function(data){
				var _data = {
					"code": "",
					"supplier": "",
					"relevant": "",
					"relevanttype": "无",
					"creator": "",
					"createtime": "",
					"finishtime": "",
					"status": "草稿",
					"deliverymethod": "",
					"address": "",
					"autopick": "",
					"target": "",
					"arrivetime": "",
					"description": "",
					"products": []
				};
				if(data != undefined) {
					for(var _k in _data){
						var _v = data[_k];
						if(_v != undefined){
							_data[_k] = _v;
						}
					}
				};
				$("#new-formcodeitem").val(_data["relevant"]);
				$("#new-formtypeitem").val(typemap[_data["relevanttype"]]);
				$("#new-deliveritem").html(_data["supplier"]);
				$("#new-descriptiontext").html(_data["description"]);
				changestatus(_data["status"]);
				var _products = _data["products"];
				if(_products){
					var _listcontent = "";
					for(var i = 0; i < _products.length; i++){
						_listcontent += "<tr>";
						for(var k in _products[i]){
							_listcontent += "<td>";
							if(k == "quantity")
								_listcontent += "<input type='text' class='span1' placeholder='出货数量'> / ";
							_listcontent += _products[i][k]; 
							_listcontent += "</td>";
						}
						_listcontent += "</tr>";
					}
					$("#new-productlist").html(_listcontent);
				}
			};

	    	var showview = function(view){
	        	$(".detail-content").css("display", "none");
	        	$(".new-content").css("display", "none");
	            $(".list-content").css("display", "none");
	            $("." + view + "-content").css("display", "block");
	            currentview = view;
	    	};
	        var showlist = function(){
	        	showview("list");
	        };
	        var showdetail = function(){
	        	showview("detail");
	        };
	        var shownew = function(){
	        	showview("new");
	        	$(".statusbar").removeClass("oe_active");
	        	$("#status-draft").addClass("oe_active");
	        	$("#detail-functionbuttongroup").css("display", "none");
				var maxcode = 0;
				for(var i = 0; i < data.length; i++){
					var _code = data[i]["code"].split("-")[1];
					_code = new Number(_code);
					if(_code > maxcode)
						maxcode = _code;
				}
				maxcode++;
	        	$("#detail-codeitem").html("OUT-000" + maxcode);
	        	fillnew();
	        };
			var lookforitem = function(code){
				var obj = null;
				for(var i = 0; i < data.length; i++)
					if(data[i]["code"] == code)
						obj = data[i];
				return obj;
			}

			var getcurrenttime = function(){
				var date = new Date().toString().split(" ");
				return date[2] + "/" + months[date[1]] + "/" + date[3] + " " + date[4];
			}

			var filllist = function(){
                var html = "";
                var items = ["code", "supplier", "relevanttype", "relevant", "creator",
                "createtime", "finishtime", "status"];
                var as = [];
                var itemlink = function(){
                	var code = $(this).attr("id");
                	$("#detail-contentobjitem").val(code);
                	filldetail(lookforitem(code));
		        	$("#showdetail").click()
                };
				for(var i = 0; i < data.length; i++){
                    html += "<tr><td><input class='list-checkbox' type='checkbox'></td>";
                    html += "<td><a href='#' id='" + data[i]["code"] + "'>" + data[i]["code"] + "</a></td>"
                    as.push(data[i]["code"]);
                    for(var j = 1; j < items.length; j++){
                    	var key = items[j];
                    	html += "<td>";
                    	html += data[i][key];
                    	html += "</td>";
                    }
                    html += "</tr>";
				}
                $("#listbody").html(html);

                for(var i = 0; i < as.length; i++)
                	$("#" + as[i]).click(itemlink);
			};

			var filldetail = function(data){
				$("#detail-contentobjitem").val(data["code"]);
				$("#detail-codeitem").html(data["code"]);
				$("#detail-statusitem").html(data["status"]);
				$("#detail-supplieritem").html(data["supplier"]);
				$("#detail-relevanttypeitem").html(data["relevanttype"]);
				$("#detail-relevantcodeitem").html(data["relevant"]);
				$("#detail-createtimeitem").html(data["createtime"]);
				$("#detail-finishtimeitem").html(data["finishtime"]);
				$("#detail-creatoritem").html(data["creator"]);
				$("#detail-deliverymethitem").html(data["deliverymethod"]);
				$("#detail-addressitem").html(data["address"]);
				$("#detail-arriveconfirmationitem").html(data["arriveconfirmation"]);
				$("#detail-targetitem").html(data["target"]);
				$("#detail-autopickitem").html(data["autopick"]);
				$("#detail-arrivetimeitem").html(data["arrivetime"]);
				$("#detail-descriptionitem").html(data["description"]);
				$("#detail-arrivetimeitem").html(data["arrivetime"]);
				if(data["status"] == "已完成" || data["status"] == "已取消"){
					$("#detail-functionbuttongroup").css("display", "none");
				} else {
					$("#detail-functionbuttongroup").css("display", "block");
				}

				changestatus(data["status"]);

				if(!data["products"]) {
					$("#detail-productlistitem").html("");
					return;
				}

				var producthtml = "";
				var productlist = ["code", "name", "quantity", "unit", "position", "pocode", "status"];
				for(var i = 0; i < data["products"].length; i++){
					var p = data["products"][i];
					producthtml += "<tr>";
					for(var j = 0; j < productlist.length; j++){
						producthtml += "<td>" + p[productlist[j]] + "</td>";
					}
					producthtml += "</tr>"
				}
				$("#detail-productlistitem").html(producthtml);

			};

			var initbuttons = function(){
		    	$("#content-title").html($("#thistitle").val());
		        $("#showlist").click(showlist);
		        $("#showdetail").click(showdetail);
		        $("#createbutton").click(shownew);
		        $("#createbutton1").click(shownew);
		        $("#discardbutton").click(showlist);
		        $("#editbutton").click(function(){
		        	var _data = lookforitem($("#detail-codeitem").html());
		        	showview("new");
		        	changestatus(_data["status"]);
		        	fillnew(_data);
		        	$("#detail-functionbuttongroup").css("display", "none");
		        });
				$("#producttaba").click(function(){
					$("#productdiv").css("display", "block");
					$("#otherinfodiv").css("display", "none");
					$("#otherinfotab").removeClass("active");
					$("#producttab").addClass("active");
				});
				$("#otherinfotab").click(function(){
					$("#productdiv").css("display", "none");
					$("#otherinfodiv").css("display", "block");
					$("#producttab").removeClass("active");
					$("#otherinfotab").addClass("active");
				});
				$("#list-head-checkbox").click(function(){
					var cbx = $(".list-checkbox").click();
				});
				$("#detail-receivebutton").click(function(){
					var obj = lookforitem($("#detail-contentobjitem").val());
					var date = new Date().toString().split(" ");
					obj["status"] = "已完成";
					obj["finishtime"] = getcurrenttime();
					filldetail(obj);
					filllist(data);
				});
				$("#detail-cancelbutton").click(function(){
					var obj = lookforitem($("#detail-contentobjitem").val());
					var date = new Date().toString().split(" ");
					obj["status"] = "已取消";
					obj["finishtime"] = getcurrenttime();
					filldetail(obj);
					filllist(data);
				});
				$("#new-producttaba").click(function(){
					$("#new-descriptiontab").removeClass("active");
					$("#new-producttab").addClass("active");
					$("#new-descriptionitem").css("display", "none");
					$("#new-productitem").css("display", "block");
				});
				$("#new-descriptiontaba").click(function(){
					$("#new-descriptiontab").addClass("active");
					$("#new-producttab").removeClass("active");
					$("#new-descriptionitem").css("display", "block");
					$("#new-productitem").css("display", "none");
				});
				$("#new-formcodeitem").typeahead({
					source: querytems,
					items: 10,
					minLength: 1,
				});
				$("#new-formcodeconfirm").click(function(){
					var _data = otherorders;
					var text = $("#new-formcodeitem").val();
					var order = null;
					for(var i = 0; i < _data.length; i++)
						if(_data[i]["code"] == text)
							order = _data[i];
					var con = {
						"relevant": order["code"],
						"supplier": order["deliver"],
						"description": order["description"],
						"relevanttype": typemap[$("#new-formtypeitem").val()],
						"products": order["products"],
					};

					fillnew(con);
				});
				$("#savebutton").click(function(){
					var neworder = {};
					neworder["code"] = $("#detail-codeitem").html();
					neworder["createtime"] = getcurrenttime();
					neworder["finishtime"] = "-";
					neworder["supplier"] = $("#new-deliveritem").html();
					neworder["relevant"] = $("#new-formcodeitem").val();
					neworder["status"] = "准备出货";
					neworder["creator"] = "Administrator";
					neworder["target"] = "深圳仓库";
					neworder["description"] = $("#new-descriptiontext").html();
					neworder["deliverymethod"] = $("#new-deliverymethitem").val();
					neworder["address"] = $("#new-addressitem").val();
					neworder["autopick"] = "是";
					neworder["relevanttype"] = typemap[$("#new-formtypeitem").val()];
					neworder["arriveconfirmation"] = typemap[$("#new-arriveconfirmationitem").val()];
					neworder["arrivetime"] = $("#new-arrivetimeitem").val();
					for(var i = 0; i < otherorders.length; i++){
						var _o = otherorders[i];
						if($("#new-formcodeitem").val() == _o["code"]){
							neworder["products"] = _o["products"];
							break;
						}
					}

					var obj = lookforitem($("#detail-codeitem").html());
					if(obj == null)
						data.push(neworder);
					else
						obj = neworder;
					filllist(data);
					filldetail(neworder);
					$("#showdetail").click();
				});
			};

            filllist(data);
            initbuttons();
            filldetail(data[0]);
	        $("#showlist").click();
		});
	</script>
	</div>
	<div id="foot">
	    <script type="text/javascript" src="../js/commonactions.js"></script>
	</div>
</body>