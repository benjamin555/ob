	<script>
	function process(processid)
	{	
		if (processid!="cancel"||confirm("您确认要取消吗？取消后本单据的状态会变为<已取消>。"))
		{		
			document.getElementById("draft").className="";
			document.getElementById("sent").className="";
			document.getElementById("confirm").className="";
			if (processid=="cancel")
			{
				document.getElementById("ulnormal").style.display="none";
				document.getElementById("ulcancel").style.display="";
			}
			else
			{
				document.getElementById("ulnormal").style.display="";
				document.getElementById("ulcancel").style.display="none";		
				document.getElementById(processid).className="oe_active";
			}
			document.getElementById("draft-process").style.display="none";
			document.getElementById("sent-process").style.display="none";
			document.getElementById("confirm-process").style.display="none";
			document.getElementById("cancel-process").style.display="none";
			document.getElementById(processid+"-process").style.display="block";
			if (processid=="confirm")
			{
				debugger;
				document.getElementById("orderlidetail").style.display="";
			}
			else
			{
				document.getElementById("orderlidetail").style.display="none";
			}
		}
	}

	function tab(tabid,modid) 
	{	debugger;
		if (modid=="detail")
		{
			document.getElementById("orderlidetail").className="";
			document.getElementById("orderdetail").className="tab-pane";						
		}
		document.getElementById("productli"+modid).className="";			
		document.getElementById("termli"+modid).className="";			
		document.getElementById("commentli"+modid).className="";			
		document.getElementById(tabid+"li"+modid).className="active";
		document.getElementById("product"+modid).className="tab-pane";			
		document.getElementById("term"+modid).className="tab-pane";			
		document.getElementById("comment"+modid).className="tab-pane";			
		document.getElementById(tabid+modid).className="tab-pane active";
	}
	</script>
