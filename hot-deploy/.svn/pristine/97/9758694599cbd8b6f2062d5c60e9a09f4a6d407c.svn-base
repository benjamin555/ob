		<script>
		function addcol(num){
			var length;
			if(num==0){
				$("<th><input class=\"input1\" type\"text\"/></th>").insertBefore("#detailthadd");
				$('#detailTbody').children().append("<td><input class=\"input1\" type\"text\"/></td>");
			}else if(num==1){
				$("<th><input class=\"input1\" type\"text\"/></th>").insertBefore("#detailthadd1");
				$('#detailTbody1').children().append("<td><input class=\"input1\" type\"text\"/></td>");
			}
		}
		
		function checkAll(){
			if ($(".ckbAll").attr("checked")) {
		        $("input:checkbox").attr("checked", true);
		    }
		    else {
		        $("input:checkbox").attr("checked", false);
		    }
		}
		
		function addrow(name){
			var rowhtml;
			if(name=="detail"){
				rowhtml = $("#detailTbody").find("tr").html();
				$("#detailTbody").append("<tr>" + rowhtml + "</tr>");
			}else if(name=="detail1"){
				rowhtml = $("#detailTbody").find("tr").html();
				$("#detailTbody1").append("<tr>" + rowhtml + "</tr>");
			}else if(name=="price"){
				rowhtml = $("#priceTbody").find("tr").html();
				$("#priceTbody").append("<tr>" + rowhtml + "</tr>");
			}else if(name=="price1"){
				rowhtml = $("#priceTbody").find("tr").html();
				$("#priceTbody1").append("<tr>" + rowhtml + "</tr>");
			}else if(name=="rollup"){
				rowhtml = $("#rollupTbody").find("tr").html();
				$("#rollupTbody").append("<tr>" + rowhtml + "</tr>");
			}else if(name=="rollup1"){
				rowhtml = $("#rollupTbody").find("tr").html();
				$("#rollupTbody1").append("<tr>" + rowhtml + "</tr>");
			}else if(name=="inventory"){
				rowhtml = $("#inventoryTbody").find("tr").html();
				$("#inventoryTbody").append("<tr>" + rowhtml + "</tr>");
			}else if(name=="inventory1"){
				rowhtml = $("#inventoryTbody").find("tr").html();
				$("#inventoryTbody1").append("<tr>" + rowhtml + "</tr>");
			}
		}
		 function del(){
			   $("input:checked").parent().parent().remove(); 	
		   };
		
		function process(processid)
		{	
			document.getElementById("draft").className="";
			document.getElementById("confirm").className="";
			if (processid=="frozen")
			{
				document.getElementById("ulnormal").style.display="none";
				document.getElementById("ulcancel").style.display="";
			}
			else
			{
				document.getElementById("ulnormal").style.display="";
				document.getElementById("ulcancel").style.display="none";		
				document.getElementById(processid).className="oe_active";
			}
			document.getElementById("draft-process").style.display="none";
			document.getElementById("confirm-process").style.display="none";
			document.getElementById("frozen-process").style.display="none";
			document.getElementById(processid+"-process").style.display="block";
		}

		
	    require(["jquery", "domReady!"], function($){
	    	// Initialize common buttons and views
	    	var currentview = "";
	    	var showview = function(view){
	        	$(".detail-content").css("display", "none");
	        	$(".new-content").css("display", "none");
	            $(".list-content").css("display", "none");
	            $(".edit-content").css("display", "none");	 
	            $("." + view + "-content").css("display", "block");
	            currentview = view;
	    	};
	        var showlist = function(){
	        	showview("list");
	        	$("#content-title").html($("#thistitle").val());
	        };
	        var showdetail = function(){
	        	showview("detail");
	        	$("#content-title").html($("#thistitle").val()+"&nbsp;&nbsp;/&nbsp;&nbsp;TY01");
	        };
	        var shownew = function(){
	        	showview("new");
	        	$("#content-title").html($("#thistitle").val());
	        };
	        var showedit = function(){
				showview("edit");
				$("#content-title").html($("#thistitle").val()+"&nbsp;&nbsp;/&nbsp;&nbsp;TY01");
		    };
	        $("#content-title").html($("#thistitle").val());
	        $("#showlist").on("click", showlist);
	        $("#showdetail").on("click", showdetail);
	        $("#createbutton").on("click", shownew);
	        $("#createbutton1").on("click", shownew);
	        $("#discardbutton").on("click", showlist);
	        $("#discardbutton1").on("click", showlist);
	        $("#savebutton").on("click", showdetail);
	        $("#savebutton1").on("click", showdetail);
	        $("#editbutton").on("click", showedit);
	        showlist();
	     	 //点击进入detail页面
	        $(".mytd").on("click", showdetail);
	       
	    });
