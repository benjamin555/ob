				$("#productInfoDiv").css("display", "none");
			});
			
			$("#productInfoTab1").on("click", function(){
				$("#remarkInfoDiv1").css("display", "none");
				$("#productInfoDiv1").css("display", "block");
			});
			$("#remarkInfoTab1").on("click", function(){
				$("#remarkInfoDiv1").css("display", "block");
				$("#productInfoDiv1").css("display", "none");
			});

			$("#productInfoTab2").on("click", function(){
				$("#remarkInfoDiv2").css("display", "none");
				$("#productInfoDiv2").css("display", "block");
			});
			$("#remarkInfoTab2").on("click", function(){
