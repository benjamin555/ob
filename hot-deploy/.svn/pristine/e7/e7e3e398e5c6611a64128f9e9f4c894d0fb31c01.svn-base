				$("#productInfoDiv2").css("display", "none");
			});
			
			$("#userInfoExam1Tab").on("click", function(){
				showdetail();
				$(".relationInfoDiv").css("display", "none");
				$(".userInfoDiv").css("display", "none");
			});
			$("#userInfoExam2Tab").on("click", function(){
				showdetail();
				$(".relationInfoDiv").css("display", "none");
				$(".userInfoDiv").css("display", "none");
			});
			$("#userInfoExam3Tab").on("click", function(){
				showdetail();
				$(".relationInfoDiv").css("display", "none");
				$(".userInfoDiv").css("display", "none");
			});
			
			$("#toSend").on("click", function(){
				$("#ulnormal").css("display", "block");
				$("#ulcancel").css("display", "none");
				document.getElementById("draft").className="";
				document.getElementById("confirm").className="oe_active";
				$("#draftDiv").css("display", "none");
				$("#confirmDiv").css("display", "block");
				$("#cancelDiv").css("display", "none");
			});
			$("#toCancel").on("click", function(){
				if(confirm("您确认要取消吗？取消后本单据的状态会变为（已取消）。")) {
					$("#ulnormal").css("display", "none");
					$("#ulcancel").css("display", "block");
					$("#draftDiv").css("display", "none");
					$("#confirmDiv").css("display", "none");
					$("#cancelDiv").css("display", "block");
				}
			});
			$("#toDraft").on("click", function(){
				$("#ulnormal").css("display", "block");
				$("#ulcancel").css("display", "none");
				document.getElementById("draft").className="oe_active";
				document.getElementById("confirm").className="";
				$("#draftDiv").css("display", "block");
				$("#confirmDiv").css("display", "none");
				$("#cancelDiv").css("display", "none");
			});
			$("#toReceive").on("click", function(){
				alert("已成功转为采购订单！");
				showlist();
			});
			$("#toReturn").on("click", function(){
